diff --git a/dts/upstream/src/arm64/amlogic/meson-g12b-a311d-cainiao-lemo-xiaoc.dts b/dts/upstream/src/arm64/amlogic/meson-g12b-a311d-cainiao-lemo-xiaoc.dts
